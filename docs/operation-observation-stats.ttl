     fhir:OperationDefinition.max [ fhir:value "1" ];
     fhir:OperationDefinition.documentation [ fhir:value "The subject of the relevant Observations, which has the value of the Observation.subject.reference. E.g. 'Patient/123'. Reference can be to an absolute URL, but servers only perform stats on their own observations" ];
     fhir:OperationDefinition.type [ fhir:value "uri" ]
  ], [
     fhir:index 1;
     fhir:OperationDefinition.name [ fhir:value "code" ];
     fhir:OperationDefinition.use [ fhir:value "in" ];
     fhir:OperationDefinition.min [ fhir:value "0" ];
     fhir:OperationDefinition.max [ fhir:value "*" ];
     fhir:OperationDefinition.documentation [ fhir:value "The test code(s) upon which the statistics are being performed. Provide along with a system, or as a coding. For example, the code  = \"45667-2\"  will evaluate all relevant Observations with this code in Observation.code. For LOINC codes that are panels, (e.g. 55284-4) this means to include all the individual codes that are part of a panel" ];
     fhir:OperationDefinition.type [ fhir:value "string" ]
  ], [
     fhir:index 2;
     fhir:OperationDefinition.name [ fhir:value "system" ];
     fhir:OperationDefinition.use [ fhir:value "in" ];
     fhir:OperationDefinition.min [ fhir:value "0" ];
     fhir:OperationDefinition.max [ fhir:value "1" ];
     fhir:OperationDefinition.documentation [ fhir:value "The system for the code(s). Or provide a coding instead" ];
     fhir:OperationDefinition.type [ fhir:value "uri" ]
  ], [
     fhir:index 3;
     fhir:OperationDefinition.name [ fhir:value "coding" ];
     fhir:OperationDefinition.use [ fhir:value "in" ];
     fhir:OperationDefinition.min [ fhir:value "0" ];
     fhir:OperationDefinition.max [ fhir:value "*" ];
     fhir:OperationDefinition.documentation [ fhir:value "The test code upon which the statistics are being performed, as a Coding" ];
     fhir:OperationDefinition.type [ fhir:value "Coding" ]
  ], [
     fhir:index 4;
     fhir:OperationDefinition.name [ fhir:value "duration" ];
     fhir:OperationDefinition.use [ fhir:value "in" ];
     fhir:OperationDefinition.min [ fhir:value "0" ];
     fhir:OperationDefinition.max [ fhir:value "1" ];
     fhir:OperationDefinition.documentation [ fhir:value "The time period of interest given as hours.  For example, the duration = \"1\" represents the last hour - the time period from on hour ago to now" ];
     fhir:OperationDefinition.type [ fhir:value "decimal" ]
  ], [
     fhir:index 5;
     fhir:OperationDefinition.name [ fhir:value "period" ];
     fhir:OperationDefinition.use [ fhir:value "in" ];
     fhir:OperationDefinition.min [ fhir:value "0" ];
     fhir:OperationDefinition.max [ fhir:value "1" ];
     fhir:OperationDefinition.documentation [ fhir:value "The time period over which the calculations to be performed, if a duration is not provided" ];
     fhir:OperationDefinition.type [ fhir:value "Period" ]
  ], [
     fhir:index 6;
     fhir:OperationDefinition.name [ fhir:value "params" ];
     fhir:OperationDefinition.use [ fhir:value "in" ];
     fhir:OperationDefinition.min [ fhir:value "1" ];
     fhir:OperationDefinition.max [ fhir:value "*" ];
     fhir:OperationDefinition.documentation [ fhir:value "average|max|min|count  The statistical operations to be performed on the relevant operations. Multiple statistics operations can be specified. These codes are defined [here](valueset-observation-statistics.html)" ];
     fhir:OperationDefinition.type [ fhir:value "code" ]
  ], [
     fhir:index 7;
     fhir:OperationDefinition.name [ fhir:value "return" ];
     fhir:OperationDefinition.use [ fhir:value "out" ];
     fhir:OperationDefinition.min [ fhir:value "1" ];
     fhir:OperationDefinition.max [ fhir:value "*" ];
     fhir:OperationDefinition.documentation [ fhir:value "A set of observations, one observation for each code, each containing one component for each statistic. The Observation.component.code contains the statistic, and is relative to the Observation.code and cannot be interpreted independently.  The Observation will also contain a subject, effectivePeriod, and code reflecting the input parameters.  The status is fixed to `final`." ];
     fhir:OperationDefinition.type [ fhir:value "Observation" ]
  ], [
     fhir:index 8;
     fhir:OperationDefinition.name [ fhir:value "source" ];
     fhir:OperationDefinition.use [ fhir:value "out" ];
     fhir:OperationDefinition.min [ fhir:value "0" ];
     fhir:OperationDefinition.max [ fhir:value "*" ];
     fhir:OperationDefinition.documentation [ fhir:value "Source observations on which the statistics are based" ];
     fhir:OperationDefinition.type [ fhir:value "Observation" ]
  ] .

